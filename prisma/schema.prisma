generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tarefa {
  @@map("Tarefas")
  id Int @id @default(autoincrement())
  nome String @unique
  custo Float
  dataLimite DateTime @db.Date
  ordemApresentacao Int @unique
}